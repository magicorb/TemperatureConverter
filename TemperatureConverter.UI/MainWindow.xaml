<Window x:Class="TemperatureConverter.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:TemperatureConverter.UI"
		xmlns:properties="clr-namespace:TemperatureConverter.UI.Properties"
		Title="{x:Static properties:Resources.MainWindowTitle}"
		Width="400" Height="200">
	<local:TemperatureConverterView
		DataContext="{Binding TemperatureConverterViewModel}"
		HorizontalAlignment="Center" VerticalAlignment="Center"/>
</Window>
