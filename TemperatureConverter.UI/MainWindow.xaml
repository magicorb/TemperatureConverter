<Window x:Class="TemperatureConverter.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:properties="clr-namespace:TemperatureConverter.UI.Properties"
		xmlns:conversion="clr-namespace:TemperatureConverter.UI.TemperatureConversion"
		Title="{x:Static properties:Resources.MainWindowTitle}"
		Width="400" Height="200">
	<conversion:TemperatureConverterView
		DataContext="{Binding TemperatureConverterViewModel}"
		HorizontalAlignment="Center" VerticalAlignment="Center"/>
</Window>
